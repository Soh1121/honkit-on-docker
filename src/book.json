{
  "root": ".",
  "plugins": [
    "anchors",
    "collapsible-chapters",
    "hide-published-with",
    "lb2br",
    "plantuml-cloud"
  ],
  "pluginsConfig": {
    "plantuml-cloud": {
      "protocol": "http",
      "host": "plantuml",
      "port": 1608,
      "blockRegex": "^```p?uml((.*[\r\n])+?)?```$"
    }
  },
  "language": "ja"
}
